{
  "name": "skillzapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "postcss": "tailwindcss -i input.css --postcss postcss.config.js --watch",
    "dev": "yarn start",
    "start": "yarn debugger && APP_ENV=development react-native start --reset-cache",
    "prebuild": "react-native prebuild --clean",
    "start:all": "APP_ENV=development react-native run-ios --ios --android --dev-client",
    "ios:build:local": "APP_ENV=development react-native run-ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios:build:eas": "eas build:run --platform ios",
    "android:build:local": "APP_ENV=development react-native run-android",
    "android:build:eas": "eas build:run --platform android",
    "eas:resignADHOC": "eas build:resign",
    "detox:android:debug:build": "detox build -c android.debug",
    "detox:android:debug:test": "detox test -c android.debug",
    "detox:android:release:build": "detox build -c android.release",
    "detox:android:release:test": "detox test -c android.release",
    "detox:ios:debug:build": "detox build -c ios.debug",
    "detox:ios:debug:test": "DETOX=true detox test -c ios.debug",
    "detox:ios:release:build": "detox build -c ios.release",
    "detox:ios:release:test": "DETOX=true detox test -c ios.release",
    "debugger": "open -g 'rndebugger://set-debugger-loc?port=8081'",
    "lint": "eslint . --fix",
    "clean": "rm -rf .expo .expo-shared ios android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-native-community/cli-platform-android": "^11.3.2",
    "@react-native-community/slider": "^4.4.2",
    "@react-native-firebase/app": "^18.0.0",
    "@react-native-firebase/messaging": "^18.0.0",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.4.0",
    "jotai": "^2.2.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.71.11",
    "react-native-autoheight-webview": "^1.6.5",
    "react-native-document-picker": "^9.0.1",
    "react-native-elements": "^3.4.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.1",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-htmlview": "^0.16.0",
    "react-native-image-slider-box": "^2.0.7",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-pager-view": "^6.2.0",
    "react-native-really-awesome-button": "^2.0.4",
    "react-native-reanimated": "^3.3.0",
    "react-native-root-toast": "^3.4.1",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.21.0",
    "react-native-segmented-control-tab": "^4.0.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^13.9.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-textinput-effects": "^0.6.3",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-native-webview": "^13.2.2",
    "react-navigation": "^4.4.4",
    "react-query": "^3.39.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-video": "^5.0.15",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
